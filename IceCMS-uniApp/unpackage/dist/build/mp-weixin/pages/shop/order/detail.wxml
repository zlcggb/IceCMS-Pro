<view class="data-v-35aaed58"><block wx:for="{{orderInfo.goods_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods-info data-v-35aaed58"><u-image vue-id="{{'19911544-1-'+index}}" width="150rpx" height="150rpx" src="{{item.cover_img}}" class="data-v-35aaed58" bind:__l="__l"></u-image><view class="info-right data-v-35aaed58"><text class="data-v-35aaed58">{{item.title}}</text><view class="price data-v-35aaed58"><text class="data-v-35aaed58">{{"￥"+item.price}}</text><text class="num data-v-35aaed58">{{"x "+orderInfo.goods_info[index].num}}</text></view></view></view></block><view class="cell data-v-35aaed58"><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">订单状态</text><text class="data-v-35aaed58">{{orderInfo.status}}</text></view><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">结算金额</text><text class="data-v-35aaed58">{{orderInfo.total_money}}</text></view><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">下单时间</text><text class="data-v-35aaed58">{{$root.f0}}</text></view></view><view class="cell data-v-35aaed58"><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">联系人</text><text class="data-v-35aaed58">{{orderInfo.address_info.name}}</text></view><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">电话</text><text class="data-v-35aaed58">{{orderInfo.address_info.moblie}}</text></view><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">地址</text><text class="data-v-35aaed58">{{orderInfo.address_info.region+orderInfo.address_info.address}}</text></view></view><block wx:if="{{orderInfo.express_company}}"><view class="cell data-v-35aaed58"><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">物流信息</text><text class="data-v-35aaed58">{{orderInfo.express_company.label}}</text></view><view class="cell-item data-v-35aaed58"><text class="data-v-35aaed58">物流单号</text><text class="data-v-35aaed58">{{orderInfo.express_number}}